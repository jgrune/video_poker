<h1>Sign In</h1>
<%= form_for @user, url: session_path, method: :post do |f| %>
  <h2><%= f.label :username %></h2>
  <%= f.text_field :username, :class => "text_field"  %><br><br>

  <h2><%= f.label :password %></h2>
  <%= f.password_field :password, :class => "text_field"  %><br><br>

  <%= f.submit "Submit", :class => "button"  %>
<% end %>
